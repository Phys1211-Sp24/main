        ",", "\"\<Intensity   Distribution\>\"", ",", "\"\<\>\""}], "}"}]}], 
     "\[IndentingNewLine]", ",", 
     RowBox[{"RotateLabel", "\[Rule]", "False"}], ",", " ", 
     RowBox[{"Axes", "\[Rule]", " ", "False"}], ",", 
     RowBox[{"PlotRange", "\[Rule]", " ", "All"}], ",", 
     RowBox[{"FormatType", "\[Rule]", " ", "TraditionalForm"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"PlotStyle", "\[Rule]", 
      RowBox[{"Hue", "[", ".7", "]"}]}]}], "}"}]}], ";"}]], "Input",
