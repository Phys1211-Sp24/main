Cell["How about a nasty one?", "Text",
 FontSize->14,ExpressionUUID->"2c1c4c38-72cd-42f2-bfd8-4141ef34c2e0"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], " ", 
  RowBox[{
   SuperscriptBox["x", "3"], 
   RowBox[{"Sin", "[", 
    FractionBox[
     RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], " ", "]"}], 
