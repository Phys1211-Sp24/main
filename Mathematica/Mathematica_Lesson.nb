Cell[CellGroupData[{

Cell["Simple Exercises:  Algebra, Derivatives, Integrals, Series", \
"SectionFirst",ExpressionUUID->"9649b4b8-d0bd-45f2-b6ca-c01e06b1c3a3"],

Cell[BoxData[
 RowBox[{"x", "=."}]], "Input",
