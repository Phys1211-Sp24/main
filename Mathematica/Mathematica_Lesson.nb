 CellLabel->"In[25]:=",ExpressionUUID->"d723e877-fc92-44b2-813d-5f5543285b3c"],

Cell["How about one you really never want to do, ever.", "Text",
 FontSize->14,ExpressionUUID->"79bc43bc-5571-4f57-b03f-3c51ed76f2af"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", 
   RowBox[{"L", "/", "64"}], "\[Omega]"], 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", " ", 
     RowBox[{"Sin", "[", 
      FractionBox[
       RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], " ", "]"}], ")"}], "13"], 
