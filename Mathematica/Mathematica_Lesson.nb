Cell["\<\
That cleared the value of x and a from above. Of course keep in mind that \
Mathematica sees x and X as different variables\
\>", "Text",
 CellChangeTimes->{
  3.7513944649377604`*^9, {3.7515591984579954`*^9, 3.7515592172112007`*^9}},
 FontSize->14,ExpressionUUID->"223fb06d-f8ae-4fcb-bf8d-39f7ef02bbc4"],

Cell[BoxData[{
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"a", " ", 
      SuperscriptBox["X", "2"]}], "+", " ", 
     RowBox[{"b", " ", "X"}], " ", "+", "c"}], " ", "\[Equal]", "0"}], ",", 
   "X"}], "]"}], "\[IndentingNewLine]", "x"}], "Input",
 CellChangeTimes->{{3.751559224269571*^9, 3.7515592258044543`*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"0e41c52a-070f-4f36-ad21-41eab37c5802"],

Cell["How about simultaneous equations?", "Text",
 FontSize->14,ExpressionUUID->"fbf55233-1364-4fd3-8f85-0a7c885e3125"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"X", "^", "2"}], " ", "+", " ", 
       RowBox[{"2", " ", "Y", " ", "X"}]}], " ", "\[Equal]", "4"}], ",", " ", 
     RowBox[{"Y", "\[Equal]", "4"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"X", ",", "Y"}], "}"}]}], "]"}]], "Input",
 CellLabel->"In[18]:=",ExpressionUUID->"a0f3f4e4-6bc7-4308-b794-954e500cf7fb"],

Cell["Here is a physics one:", "Text",
 FontSize->14,ExpressionUUID->"173434fd-96a1-4ec2-8a58-f708f90fef4f"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[ScriptCapitalE]squared", " ", "\[Equal]", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"p", " ", "c"}], ")"}], "2"], " ", "+", "  ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"m", " ", 
          SuperscriptBox["c", "2"]}], ")"}], "2"]}]}], ",", 
     RowBox[{"p", "==", " ", 
      FractionBox[
       RowBox[{"m", " ", "v"}], 
       SqrtBox[
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["v", "2"], "/", 
          SuperscriptBox["c", "2"]}]}]]]}]}], "}"}], ",", "v"}], 
  "]"}]], "Input",
 CellLabel->"In[19]:=",ExpressionUUID->"5c94d1bf-216d-4dcf-b2b2-6187e07cc275"],

Cell[TextData[{
 "Try on the top tool bar",
 StyleBox[" Insert \[Rule] Special Characters  ",
  FontWeight->"Bold"],
 "to insert some special characters\nWe will take a partial derivative "
}], "Text",
 CellChangeTimes->{{3.7513945107292795`*^9, 3.7513945366948533`*^9}, {
  3.794782843070367*^9, 3.794782859373801*^9}},
 FontSize->14,ExpressionUUID->"958e646d-4b73-4e93-9286-56aae56eb15c"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "\[Beta]"], " ", 
  RowBox[{"(", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", " ", "\[Beta]"}], " ", 
     SubscriptBox["\[ScriptCapitalE]", "n"]}]], ")"}]}]], "Input",
 CellLabel->"In[20]:=",ExpressionUUID->"905f312e-fc76-407c-b23c-ca9a744980e0"],

Cell[TextData[{
 "Again ",
 StyleBox["Insert \[Rule] Special Characters ",
  FontWeight->"Bold"],
 "allows you to insert special characters, but there are also keyboard \
shortcuts for most of them:\n",
 StyleBox["esc ee  esc", "Input"],
 " = \[ExponentialE]\n",
 StyleBox["esc inf esc ", "Input"],
 "= \[Infinity]  \n",
 StyleBox["esc e esc", "Input"],
 " =  \[Epsilon] \n",
 StyleBox["esc p esc", "Input"],
 " = \[Pi]\n\n Define a function (note the underscore for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Beta]", "_"], TraditionalForm]],ExpressionUUID->
  "cc1aa8e2-ecf3-4117-af7c-6e391192cee3"],
 " \n that's a little weird thing in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " )"
}], "Text",
 CellChangeTimes->{{3.751394563655759*^9, 3.7513947404589796`*^9}, {
   3.7513948040389752`*^9, 3.751394844875775*^9}, 3.7947828755575633`*^9},
 FontSize->14,ExpressionUUID->"2fdcb0d4-6ae3-426c-8111-b8dfa766b655"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Z", "[", 
    RowBox[{"\[Beta]_", ",", "\[ScriptCapitalN]_"}], "]"}], "=", 
   RowBox[{"-", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], "\[ScriptCapitalN]"], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[Beta]"}], " ", 
       SubscriptBox["\[Epsilon]", "n"]}]]}]}]}], ";"}]], "Input",
 CellLabel->"In[93]:=",ExpressionUUID->"0d111892-a7db-4c1c-9ba3-cb6fafc87fa2"],

Cell[BoxData[
 RowBox[{"Z", "[", 
  RowBox[{"10", ",", "1"}], "]"}]], "Input",
 CellLabel->"In[94]:=",ExpressionUUID->"9080e9a1-7996-4bad-9968-125cb3525503"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "\[Beta]"], 
  RowBox[{"Log", "[", 
   RowBox[{"Z", "[", 
    RowBox[{"\[Beta]", ",", "\[Infinity]"}], "]"}], "]"}]}]], "Input",
 CellLabel->"In[95]:=",ExpressionUUID->"91bf4ad5-d420-4c57-a23a-86c9c18a9313"],

Cell["Mathematica integrate functions numerically and analytically", "Text",
 CellChangeTimes->{{3.7513948624338284`*^9, 3.751394864735709*^9}, {
  3.7515593151277313`*^9, 3.751559348585677*^9}},
 FontSize->14,ExpressionUUID->"36aebdec-e9b8-4975-a8d0-6ea84c6fea8a"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Pi]"], 
  RowBox[{
   RowBox[{"Sin", "[", 
    FractionBox[
     RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], " ", "]"}], 
