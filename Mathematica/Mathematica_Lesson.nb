Cell[TextData[{
 "Suppose you wish to plot a simple trig function. Simply type the following \
\nand hit enter (notice the \"cell\" brackets to the very far left in blue. ",
 StyleBox["Everything \nhas its own \"cell\".",
  FontWeight->"Bold"],
 " Double click on the various brackets to see what happens, you can\n\
\"minimize\" sections by doing this.)"
}], "Text",
 CellChangeTimes->{{3.7513938286005435`*^9, 3.7513938299125614`*^9}},
 FontSize->14,ExpressionUUID->"9db6327a-1812-4935-899c-8d82d71ff8ef"],

Cell[BoxData[
 RowBox[{
  RowBox[{"P1", "=", 
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"Sin", "[", "x", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", 
       RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
     RowBox[{"PlotStyle", "\[Rule]", 
      RowBox[{"Hue", "[", ".6", "]"}]}]}], "]"}]}], ";"}]], "Input",
 CellLabel->"In[1]:=",ExpressionUUID->"7c428f02-4535-403b-ac4e-5dfdf363f325"],

